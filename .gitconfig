[alias]
        ifetch = fetch -v --all
        amend = commit -a --amend
        ls = log --pretty=format:"%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate
        graph = log --graph --abbrev-commit --decorate --date=relative --all
        branch-status = !"git ifetch >> /dev/null && git branch -v"
